{
  "dashboard": {
    "id": null,
    "title": "Liberation Stack Overview",
    "tags": ["liberation", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Liberation Services Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=~\"liberation-.*\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Database Connections",
        "type": "graph",
        "targets": [
          {
            "expr": "pg_stat_database_numbackends{datname=\"liberation_main\"}",
            "legendFormat": "PostgreSQL Connections"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Vector Store Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "qdrant_collections_vector_total",
            "legendFormat": "Total Vectors"
          },
          {
            "expr": "liberation_ai_vectors_total",
            "legendFormat": "AI Service Vectors"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "liberation_ai_memory_bytes",
            "legendFormat": "Liberation AI Memory"
          },
          {
            "expr": "container_memory_usage_bytes{name=~\".*liberation.*\"}",
            "legendFormat": "{{name}} Container Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 5,
        "title": "Redis Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(redis_commands_total[5m])",
            "legendFormat": "Commands/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 6,
        "title": "Container CPU Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(container_cpu_usage_seconds_total{name=~\".*liberation.*\"}[5m]) * 100",
            "legendFormat": "{{name}} CPU %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      }
    ]
  }
}